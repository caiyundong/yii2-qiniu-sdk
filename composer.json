{
  "name": "caiyundong/yii2-qiniu-sdk",
  "version": "1.0.1",
  "type": "yii2-extension",
  "description": "Qiniu API for Yii 2.0",
  "keywords": ["yii2", "qiniu", "upload", "sdk", "bucket"],
  "homepage": "https://github.com/caiyundong/yii2-qiniu-sdk",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/caiyundong/yii2-qiniu-sdk/issues",
    "wiki": "https://github.com/caiyundong/yii2-qiniu-sdk/wiki",
    "source": "https://github.com/caiyundong/yii2-qiniu-sdk"
  },
  "authors": [
    {
      "name": "caiyundong",
      "email": "yundong.cai@gmail.com",
      "homepage": "https://github.com/caiyundong",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "qiniu/php-sdk": "v7.2.6"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.0"
  },
  "autoload": {
    "psr-4": {
      "caiyundong\\Qiniu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "caiyundong\\Qiniu\\Tests\\": "tests/"
    }
  }
}